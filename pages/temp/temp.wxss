/*活动标题*/
.start-title{
  background: #fff;
  line-height: 82rpx;
  border-bottom: 1rpx solid #f3f3f3;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding: 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-align-items: center;
	-ms-flex-item-align:center;
	align-items: center;
}
.start-input{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	-ms-flex: 1;
	flex: 1;
	width: 0;

}
.strat-name{
    padding-right: 20rpx;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.start-txt{
    margin: 30rpx;
    background: #fff;
    -webkit-border-raduis:10rpx;
    border-radius: 10rpx;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding: 30rpx;
    height: 260rpx;
    width: 92%;
}
/*高级设置*/
.start-set{
 text-align: center;
 color: #fe784c;
 font-size: 32rpx;
 text-decoration: underline;
}

/*邀请范围*/
.invite{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding: 0 30rpx;
	-webkit-justify-content: space-between;
	justify-content:space-between;
    background: #fff;
    -webkit-border-radius:10rpx;
    border-radius: 10rpx;
    margin: 20rpx 30rpx;
    height: 90rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    color: #666;
}

/*提交*/
.s-submit{
    text-align: center;
    background: #fe784c;
    -webkit-border-radius:10rpx;
    border-radius: 10rpx;
    color: #fff;
    line-height: 80rpx;
    margin: 30rpx;
}
.uploadImage{
    color: ebac40;
}




/*补充说明*/
.add{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.add-btn{
    width: 50%;
    margin: 0 auto;
    margin-bottom: 10px;
}
.add-btn button{
    background: #fe784c;
}
